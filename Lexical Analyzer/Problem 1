#include <iostream>
using namespace std;

bool isNum(string s) {
    int dotCount = 0;
    for (int i = 0; s[i] != '\0'; i++) {
        if (s[i] == '.') {
            dotCount++;
            if (dotCount > 1)
                return false;
        } else if (s[i] < '0' || s[i] > '9') {
            return false;
        }
    }
    return s.length() > 0 && s != ".";
}

int main() {
    string s;
    cout << "Enter input: ";
    cin >> s;

    if (isNum(s)) {
        cout << "Numeric constant" << endl;
    } else {
        cout << "Not numeric" << endl;
    }

    return 0;
}
